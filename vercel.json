{
  "version": 2,
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/create-react-app"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.js",
      "methods": ["GET", "POST", "PUT", "PATCH", "OPTIONS", "DELETE"],
      "headers": [{ "key": "Access-Control-Allow-Origin", "value": "*" }]
    }
  ]
}
